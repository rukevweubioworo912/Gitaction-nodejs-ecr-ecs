{
  "family": "nodejs-app-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::339712938001:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "nodejs-app",
      "image": "339712938001.dkr.ecr.us-east-1.amazonaws.com/nodejs-app-design:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
